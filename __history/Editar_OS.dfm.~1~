  object QRY_ITENS_ADD: TFDQuery
    Connection = DM.FDConnection1
    SQL.Strings = (
      
        'SELECT ID.IDOPERACOES, OP.CONTRATO,ID.IDADICIONAIS,AD.DESCRICAO,' +
        ' ID.PONTUACAO FROM OPERACOES OP, adicionais AD, itens_adicionais' +
        ' ID'#10'WHERE ID.IDADICIONAIS = AD.IDADICIONAIS AND ID.IDOPERACOES =' +
        ' OP.IDOPERACOES ')
    Left = 544
    Top = 392
    ParamData = <
      item
        Name = 'ID'
      end>
  end
